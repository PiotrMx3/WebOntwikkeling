<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Pets</title>
</head>

<body>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Age</th>
                <th>Type</th>
                <th>Breed</th>
            </tr>
        </thead>
        <tbody>
            <% animals.forEach(animal=> { %>
                <tr>

                    <td>
                        <%= animal.name %>
                    </td>
                    <td>
                        <%= animal.age %>
                    </td>
                    <td>
                        <%= animal.type %>
                    </td>
                    <td>
                        <%= animal.breed %>
                    </td>
                </tr>
                <% }) %>

        </tbody>
    </table>
</body>

</html>